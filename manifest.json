{
    "author": "Laurin Neff",
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "css": [
                "blacktheme.css"
            ]
        }
    ],
    "description": "Changes the YouTube Website to have a black theme. Disable this extension to get the original theme.",
    "icons": {
        "144": "icons/blackyt-144.png"
    },
    "manifest_version": 2,
    "name": "YTBlackTheme",
    "version": "1.0.1",
    "-ms-preload": {
        "backgroundScript": "backgroundScriptsAPIBridge.js",
        "contentScript": "contentScriptsAPIBridge.js"
    }
}